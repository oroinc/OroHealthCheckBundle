{
    "name": "oro/health-check-bundle",
    "description": "Oro Health Check Bundle",
    "homepage": "https://github.com/oroinc/OroHealthCheckBundle.git",
    "keywords": ["Oro", "Health Check"],
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.oroinc.com"
        }
    ],
    "require": {
        "oro/platform-enterprise": "5.0.*",
        "liip/monitor-bundle": "2.*",
        "laminas/laminas-diagnostics": "1.15.*"
    },
    "autoload": {
        "psr-4": { "Oro\\Bundle\\HealthCheckBundle\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
